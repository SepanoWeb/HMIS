        <script>
            $(function () {
            });
            $(".ShowOneDiv").click(function () {
                showOneDiv(this);

            });
            var backArray = [];
            var showOneDiv = function (obj) {
                if ($(obj).attr('href') != null) {
                    backArray.length = 0;
                    backArray.push($(obj).attr('href'));
                }
                //                alert(backArray);
                $(".Alldiv").hide();
                var href = $(obj).attr('href');
                if (!href) {
                    $(obj).show(); // برای مواقعی که بعضی دیو ها لینک ندارند به جای این که لینک را نمایش دهد خود فرم را نمایش  دهد
                } else {
                    $(href).show(); //و اگر که لینک بود خودش را نمایش بده
                }
            };
            var switchOneDiv = function (obj) {
                $(".Alldiv").hide();
                $(obj).show();
            };
            var goBack = function (obj) {

                if (backArray.length != 0)
                    id = backArray.pop();
                if (id !== null && id != "" && id != "undefined") {

                    showOneDiv($(id));
                }

            };
